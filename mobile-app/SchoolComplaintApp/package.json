  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start-safe": "expo start --clear --port 8082",
    "start-clean": "npm run clean && expo start --clear",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "kill-ports": "lsof -ti :8081 | xargs kill -9 2>/dev/null || true && lsof -ti :8082 | xargs kill -9 2>/dev/null || true",
    "test-auth": "echo '🧪 인증 시스템 테스트 시작' && npm run start-safe"
  },
